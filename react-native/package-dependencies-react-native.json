{
  "devDependencies": {
    "@eslint/compat": "^0.1.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^3.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:web": "eslint src/web --ext .ts,.tsx,.js,.jsx",
    "lint:native": "eslint src/native --ext .ts,.tsx,.js,.jsx"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "description": "Dépendances pour la configuration globale ESLint + Prettier React Native avec Expo",
  "usage": "Copier ce fichier dans votre projet et exécuter 'npm install' ou 'yarn install'"
}
